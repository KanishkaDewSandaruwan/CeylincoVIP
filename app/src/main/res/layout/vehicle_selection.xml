<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/artboard_1">

    <ImageView
        android:id="@+id/trns_logo"
        android:layout_width="146dp"
        android:layout_height="129dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="55dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/vip_logo1" />

    <TextView
        android:id="@+id/chooseVehicle"
        android:layout_width="376dp"
        android:layout_height="46dp"
        android:fontFamily="sans-serif-medium"
        android:text="Choose Your Vehicle"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="27sp"
        android:textStyle="bold"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/trns_logo"
        app:layout_constraintVertical_bias="0.022" />

    <ImageButton
        android:id="@+id/cycleImg"
        android:layout_width="126dp"
        android:layout_height="79dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chooseVehicle"
        app:srcCompat="@drawable/asset_5"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/doserImg"
        android:layout_width="141dp"
        android:layout_height="76dp"
        android:layout_marginTop="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toEndOf="@+id/tractImg"
        app:layout_constraintTop_toBottomOf="@+id/busimg"
        app:layout_constraintVertical_bias="0.191"
        app:srcCompat="@drawable/asset__3" />

    <ImageView
        android:id="@+id/busimg"
        android:layout_width="190dp"
        android:layout_height="66dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toEndOf="@+id/lorryImg"
        app:layout_constraintTop_toBottomOf="@+id/vanImg"
        app:srcCompat="@drawable/asset__1" />

    <ImageView
        android:id="@+id/wheelImg"
        android:layout_width="125dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.964"
        app:layout_constraintStart_toEndOf="@+id/cycleImg"
        app:layout_constraintTop_toBottomOf="@+id/chooseVehicle"
        app:srcCompat="@drawable/asset_1" />

    <ImageView
        android:id="@+id/tractImg"
        android:layout_width="145dp"
        android:layout_height="84dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lorryImg"
        app:layout_constraintVertical_bias="0.083"
        app:srcCompat="@drawable/asset__2" />

    <ImageView
        android:id="@+id/vanImg"
        android:layout_width="134dp"
        android:layout_height="72dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toEndOf="@+id/carImg"
        app:layout_constraintTop_toBottomOf="@+id/wheelImg"
        app:srcCompat="@drawable/asset_6" />

    <ImageView
        android:id="@+id/lorryImg"
        android:layout_width="132dp"
        android:layout_height="81dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carImg"
        app:srcCompat="@drawable/asset_4" />

    <ImageView
        android:id="@+id/carImg"
        android:layout_width="128dp"
        android:layout_height="96dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cycleImg"
        app:srcCompat="@drawable/asset_2" />


</androidx.constraintlayout.widget.ConstraintLayout>
